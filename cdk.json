{
  "app": "python3 app.py",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "source.bat",
      "**/__init__.py",
      "python/__pycache__",
      "tests"
    ]
  },
  "context": {
    "@aws-cdk/core:newStyleStackSynthesis": true,
    "@aws-cdk-lib/aws-apigateway:usagePlanKeyOrderInsensitiveId": true,
    "@aws-cdk-lib/aws-rds:lowercaseDbIdentifier": true,
    "@aws-cdk-lib/aws-lambda:recognizeVersionProps": true,
    "@aws-cdk-lib/aws-cloudfront:defaultSecurityPolicyTLSv1.2_2021": true,
    "python_version": "3.8",
    "short_url_host": "link.mindtether.rudy.fyi", 
    "api_host": "api.mindtether.rudy.fyi",
    "prod":{
      "api_host":"api.mindtether.app",
      "stage": "v1",
      "short_url_host": "link.mindtether.app"
    },
    "dev":{
      "api_host":"api.mindtether.app",
      "stage": "dev",
      "short_url_host": "dev.link.mindtether.app"
    }
    
  }
}
